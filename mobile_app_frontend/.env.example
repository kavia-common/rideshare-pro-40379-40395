# PUBLIC INTERFACE: Environment variables required by the mobile_app_frontend
# Copy this file to .env and set values for your environment.
# Note: CRA only exposes variables prefixed with REACT_APP_

REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000

# Optional additional settings used by platform
REACT_APP_FRONTEND_URL=
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=
