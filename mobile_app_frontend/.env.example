# Mobile App Frontend (React) - .env.example
# Copy to .env and adjust values. CRA only exposes REACT_APP_* at build time.

# Base API URL (http(s)://host:port)
REACT_APP_API_BASE=http://localhost:4000
# Backend canonical URL, used as fallback by the app
REACT_APP_BACKEND_URL=http://localhost:4000
# Frontend URL (used by backend CORS and some diagnostics)
REACT_APP_FRONTEND_URL=http://localhost:3000
# Socket.IO server URL (http(s)://host:port)
REACT_APP_WS_URL=http://localhost:4000

# Environment indicators
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1

# Local dev port for CRA
REACT_APP_PORT=3000

# Proxy/trust and logging
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=dev

# Healthcheck path on backend
REACT_APP_HEALTHCHECK_PATH=/health

# JSON strings for feature toggles/experiments (optional)
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=0
