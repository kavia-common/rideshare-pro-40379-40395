# Backend API Server - Environment Variables Example
# Copy this file to .env and set values accordingly.

# Required: MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/rideshare_pro

# Optional: Server configuration
PORT=4000
TRUST_PROXY=1
LOG_LEVEL=dev

# CORS/Frontend integration (used for CORS and documentation)
REACT_APP_API_BASE=http://localhost:4000
REACT_APP_BACKEND_URL=http://localhost:4000
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:4000
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=0
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=1
REACT_APP_LOG_LEVEL=dev
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=0
